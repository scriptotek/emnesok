{
  "name": "emnesok",
  "version": "0.1.0",
  "author": "UiO Realfagsbiblioteket",
  "description": "Realfagsbibliotekets emnesÃ¸k",
  "license": "MIT",
  "homepage": "https://github.com/scriptotek/emnesok#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scriptotek/emnesok.git"
  },
  "bugs": {
    "url": "https://github.com/scriptotek/emnesok/issues"
  },
  "scripts": {
    "transifex:push": "node scripts/transifex push",
    "transifex:pull": "node scripts/transifex pull",
    "update-pot": "angular-gettext-cli --files 'src/app/**/*.+(js|html)' --dest 'src/po/emnesok.pot' && yarpm run transifex:push",
    "compile-po": "yarpm run transifex:pull && angular-gettext-cli --compile --files 'src/po/**/*.po' --format json --dest src/app/services/translations.json",
    "trans": "yarpm run compile-po && yarpm run update-pot",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js",
    "build": "NODE_ENV=production webpack --config webpack/webpack.config.prod.js --colors --progress"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "warn"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 0.25%, not dead"
        }
      ]
    ],
    "plugins": [
      [
        "angularjs-annotate",
        {
          "explicitOnly": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "angular-gettext-cli": "^1.2.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-angularjs-annotate": "^0.9.0",
    "browser-sync": "^2.24.7",
    "browserify": "^16.2.2",
    "browserslist": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "connect-history-api-fallback": "^1.1.0",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "ejs-loader": "^0.3.1",
    "eslint": "^5.4.0",
    "eslint-loader": "^2.1.0",
    "extract-loader": "^3.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "loglevel": "^1.6.1",
    "mini-css-extract-plugin": "^0.4.2",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "transifex-api-es6": "^0.2.3",
    "ts-loader": "^5.2.1",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "yarpm": "^0.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "angucomplete-alt": "~2.5.0",
    "angular": "^1.7.4",
    "angular-animate": "^1.7.4",
    "angular-gettext": "~2.4.1",
    "angular-jsonld": "~0.2.0",
    "angular-loading-bar": "~0.9.0",
    "angular-sanitize": "^1.7.4",
    "angular-ui-bootstrap": "~2.5.6",
    "angular-ui-router": "~1.0.20",
    "angulartics": "~1.6.0",
    "angulartics-google-analytics": "~0.5.0",
    "bootstrap": "~3.3.7",
    "jsonld": "~0.4.11",
    "lodash": "^4.17.10"
  }
}
